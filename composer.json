{
    "name": "prestashop/mjml-theme-converter",
    "license": "OSL-3.0",
    "type": "project",
    "autoload": {
        "psr-4": {
            "AppBundle\\": "src/AppBundle"
        },
        "classmap": [
            "app/AppKernel.php",
            "app/AppCache.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=8.1",
        "ext-dom": "*",
        "doctrine/doctrine-bundle": "^2.3",
        "doctrine/orm": "^2.15",
        "incenteev/composer-parameter-handler": "^2.3",
        "symfony/console": "6.4.*",
        "symfony/css-selector": "~6.4",
        "symfony/debug-bundle": "~6.4",
        "symfony/dom-crawler": "~6.4",
        "symfony/dotenv": "6.4.*",
        "symfony/error-handler": "~6.4",
        "symfony/flex": "^2",
        "symfony/framework-bundle": "6.4.*",
        "symfony/monolog-bundle": "^3.7.0",
        "symfony/runtime": "6.4.*",
        "symfony/twig-bundle": "~6.4.0",
        "symfony/yaml": "6.4.*",
        "twig/twig": "^3.0"
    },
    "require-dev": {
        "symfony/maker-bundle": "^1.65"
    },
    "scripts": {
        "symfony-scripts": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters"
        ],
        "post-install-cmd": [
            "@symfony-scripts"
        ],
        "post-update-cmd": [
            "@symfony-scripts"
        ],
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "symfony/flex": true,
            "symfony/runtime": true
        }
    },
    "extra": {
        "symfony": {
            "require": "~6.4"
        },
        "symfony-app-dir": "app",
        "symfony-bin-dir": "bin",
        "symfony-var-dir": "var",
        "symfony-web-dir": "web",
        "symfony-tests-dir": "tests",
        "symfony-assets-install": "relative",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        }
    }
}
